    "typescript": "^2.4.2",
    "uglify-js": "^3.0.28",
    "update-notifier": "^2.2.0"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/__mocks__/setup.js"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
