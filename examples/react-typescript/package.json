  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "fuse-box": "^2.2.2",
    "fuse-box-typechecker": "^2.6.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^16.0.0",
    "run-sequence": "^2.1.0",
    "ts-jest": "^21.0.1",
